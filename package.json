{
  "name": "ferrisscript",
  "version": "0.0.2",
  "description": "FerrisScript - A Rust-powered scripting language for Godot",
  "private": true,
  "scripts": {
    "docs:lint": "markdownlint \"**/*.md\" --ignore node_modules --ignore target --ignore extensions/vscode/node_modules --dot",
    "docs:fix": "markdownlint \"**/*.md\" --ignore node_modules --ignore target --ignore extensions/vscode/node_modules --dot --fix"
  },
  "devDependencies": {
    "markdown-link-check": "^3.12.2",
    "markdownlint-cli": "^0.41.0"
  }
}
